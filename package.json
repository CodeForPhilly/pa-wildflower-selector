{
  "name": "pa-wildflower-selector",
  "version": "0.1.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "dev": "PORT=3001 node app & vue-cli-service serve",
    "ssr-dev": "npm run build && node app",
    "lint": "vue-cli-service lint",
    "build-browser": "node scripts/build-browser.js",
    "build-ssr": "node scripts/build-ssr.js",
    "build": "npm run build-ssr && npm run build-browser",
    "deploy-to-staging": "npm run build && sc-deploy staging",
    "sign-secrets": "./script/sign-secrets",
    "update-data": "node download && node massage",
    "fast-update-data": "node download --skip-images && node massage",
    "generate-embeddings": "node scripts/generate-embeddings.js",
    "sync-images": "node scripts/sync-images.js",
    "sync:down": "node scripts/sync-down.js",
    "sync:down:db": "node scripts/sync-down.js --skip-images",
    "sync:down:images": "node scripts/sync-down.js --images-only",
    "sync:up": "node scripts/sync-up.js",
    "sync:up:db": "node scripts/sync-up.js --skip-images",
    "sync:up:images": "node scripts/sync-up.js --images-only",
    "scrape-wikimedia": "node scripts/scrape-wikimedia-images.js",
    "docker:up": "docker compose up -d --build",
    "docker:down": "docker compose down",
    "predev:local": "node scripts/dev-setup.js",
    "dev:local": "npm run predev:local && concurrently --names \"SERVER,CLIENT\" --prefix-colors \"blue,green\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vue-cli-service serve",
    "dev:server": "cross-env NODE_ENV=development node app.js",
    "dev:full": "concurrently --names \"SERVER,CLIENT\" --prefix-colors \"blue,green\" \"npm run dev:server\" \"npm run dev:client\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@vue/cli-plugin-babel": "^4.5.16",
    "@vue/cli-plugin-eslint": "^4.5.16",
    "@vue/cli-service": "^4.5.16",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/server-renderer": "^3.2.31",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.7.7",
    "babel-eslint": "^10.1.0",
    "boring": "^1.1.1",
    "cheerio": "^1.0.0-rc.9",
    "core-js": "^3.21.1",
    "csv-parse": "^4.15.4",
    "dotenv": "^16.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.20.0",
    "express": "^4.17.3",
    "google-spreadsheet": "^5.0.2",
    "leaflet": "^1.7.1",
    "limiter": "^2.1.0",
    "lucide-vue-next": "^0.562.0",
    "mongodb": "^3.6.7",
    "mongodb-mcp-server": "^1.2.0",
    "node-fetch": "^2.6.1",
    "qs": "^6.10.2",
    "regexp-quote": "0.0.0",
    "sluggo": "^0.3.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-node-externals": "^3.0.0",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0",
    "@vue/cli-plugin-typescript": "^4.5.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "ts-loader": "^8.4.0",
    "typescript": "^5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "alias": {
    "vue": "./node_modules/vue/dist/vue.esm.js"
  },
  "bin": {
    "vue-cli-service": "./node_modules/.bin/vue-cli-service"
  }
}
