{
  "name": "pa-wildflower-selector",
  "version": "0.1.1",
  "private": true,
  "main": "app.js",
  "scripts": {
    "dev": "PORT=3001 node app & vue-cli-service serve",
    "dev-win": "cross-env PORT=3001 concurrently \"node app\" \"vue-cli-service serve\"",
    "ssr-dev": "npm run build && node app",
    "lint": "vue-cli-service lint",
    "build-browser": "rimraf dist node_modules/.cache && vue-cli-service build && xcopy /E /Y dist\\* public\\ && rimraf public\\index.html dist\\",
    "build-ssr": "rimraf dist node_modules/.cache && SET SSR=true && vue-cli-service build && rimraf ssr && move dist ssr",
    "build": "npm run build-ssr && npm run build-browser",
    "deploy-to-staging": "npm run build && sc-deploy staging",
    "sync-up": "./scripts/sync-up",
    "sign-secrets": "./script/sign-secrets",
    "upload-test-data": "./scripts/upload-test-data",
    "restore-test-data": "./scripts/restore-test-data",
    "restore-test-data-win": "./scripts/restore-test-data.js",
    "update-data": "node download && node massage",
    "fast-update-data": "node download --skip-images && node massage"
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.3.12",
    "@vue/server-renderer": "^3.3.12",
    "boring": "^1.1.1",
    "cheerio": "^1.0.0-rc.12",
    "child_process": "^1.0.2",
    "core-js": "^3.34.0",
    "csv-parse": "^5.5.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "express": "^4.18.2",
    "google-spreadsheet": "^4.1.1",
    "https": "^1.0.0",
    "leaflet": "^1.9.4",
    "limiter": "^2.1.0",
    "mongodb": "^6.3.0",
    "node-fetch": "^2.7.0",
    "qs": "^6.11.2",
    "regexp-quote": "0.0.0",
    "sluggo": "^1.0.0",
    "tar": "^6.2.0",
    "vue": "^3.3.12",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-node-externals": "^3.0.0",
    "yaml": "^2.3.4",
    "zlib": "^1.0.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off",
      "vue/no-reserved-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "alias": {
    "vue": "./node_modules/vue/dist/vue.esm.js"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/eslint-parser": "^7.23.3",
    "@vue/cli-service": "^5.0.8",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  }
}
